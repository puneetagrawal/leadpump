
<div class="container wrapper">
    <div class="row-fluid">
        <div class="span12">
           <div class="logo_listing m_item_main">
            <%if @companymallitem.present?%>
            <h2 style="text-align:center;">Download or Print Coupons here.</h2>
            <%@companymallitem.each do|ol|%>
            <%mall = Onlinemall.find(ol.onlinemall_id)%>
                <div class="logo_con ">
                    <h3 title="<%=mall.title%>"><%=mall.title%></h3>
                    <%if mall.mallpic.present?%>
                    <%= image_tag mall.mallpic.last.avatar.url(:medium), :style=>"max-height:75px;max-width:100px" %>
                    <%else%>
                    <img src="/assets/not-found.png" style="max-height:75px;max-width:100px">
                    <%end%>
                    <div class="action_btn viewmallitem" id="view_<%=mall.id%>">
                        <%=link_to "javascript:void(0)" ,:class=>"btn yellow pull-left" do%>   
                        <i class="icon-download" ></i>
                        Download or Print
                        <%end %>
                    </div>
                </div>
            <%end%>
            <%else%>
                <h2 style="text-align:center;">Sorry! No items presents.</h2>
            <%end%>
            </div>
        </div>
    </div>
</div>

