<table class="table table-striped">
    <thead>
        <tr>
            <td>VIP Name</td>
            <td>Associate</td>
            <td>VIP Phone Number</td>
            <td>VIP Status</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        <%if @leads.present? %>
            <% @leads.each do |lead|%>
                <%if lead.present?%>
                    <%=render "vipleadContentRow", :lead=>lead%>
                <%end%>
            <% end %>
        <% else  %>
            <tr>
                <td colspan="5">No record found.</td>
            </tr>
        <% end %>
    </tbody>
</table>
