
       
<header>
    <div class="container">
        <h2 class="fl"><a href="#">Company logo</a></h2>
        <div class="fr">
            <ul class="fr usernav">
                <li> 
                <%- if current_user %>
                <%=link_to(destroy_user_session_path, html_options = {}) do%>
                  <i class="icon-chevron-right icon-white icon_login"></i>
                  Logout
                <%end %>
                <%else%>
                    <%=link_to("users/sign_in", html_options = {}) do %>
                      <i class="icon-chevron-right icon-white icon_login"></i>
                      Login
                    <%end %>
                <%end%>
                </li>
                
            </ul>
            <div class="clear"></div>
            <div id="headerNav" class="navbar">
                <div class="navbar-inner">
                    <div class="container">
                        <a class="btn yellow btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </a>
                        <div class="nav-collapse collapse fr">
                            <ul class="nav">
                                <li><%= link_to "Home", home_index_path, :class => "active" %></li>
                                <li><%= link_to "Plan", "#" %></li>
                                <li><%= link_to "About", "#" %></li>
                                <li><%= link_to "Contact us", "#" %></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>





