<div class="container">
    <div class="row-fluid">
        <div class="span12">
           <div class="border">
                <table class="table table-striped">
                    <thead>
                    <tr><td>Name</td><td>Email</td><td>Plan Type</td><td>Plan Name</td><td>Payment</td><td># of Users</td></tr>
                    </thead>
                     	<% @users.each do |user| %>
                     	<% if user.subscription.present? %>
                    <tbody>
                    <tr> <td><%= user.name %></td> 
                  		 <td><%= user.email %></td>
                    	 <td><%= user.subscription.plan_type %> </td>
                         <td> <%=user.subscription.plan_per_user_range.plan.name%> </td>
                         <td> <%= user.subscription.payment %> </td>
                         <td> <%= user.subscription.users_count %> </td>
                     </tr>
                    </tbody>
                    <% end %>
                    <% end %>
                </table>
            </div>
        </div>
    </div>
</div>

