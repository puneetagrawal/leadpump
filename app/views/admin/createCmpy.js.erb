<%if @error.blank?%>
	$(".table-striped tbody tr:first").before('<%=escape_javascript(render partial: "userrow", :locals=>{:user=>@user})%>')
	$(".comp").html('<%=@companycount%>')
	alert("Company created successfully")
	$.fancybox.close();
<%else%>
	$(".fancybox-inner .error").html('<%=@error%>');
	$('.fancybox-inner .createCmpy').html('<input type="button" name="submitCmpy" value="Create" class="btn yellow">');
<%end%>
