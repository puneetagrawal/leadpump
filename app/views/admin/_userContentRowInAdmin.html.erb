<% if @users.present? %>
	<% @users.each do |user| %>
		<tbody>
			<tr id="viewLead_<%= user.id %>"> 
				<td><%= user.name.humanize %></td> 
				<td><%=user.role.role_type%></td>
				<td><%=user.fetchPlanName%>
				</td>
				<td class="leadActive leadAction" id="status_<%=user.id%>">
					<span class="span leadAction"><%=user.active ? 'Active' : 'Inactive'%></span>
				</td>
				<td>
					<%= link_to "Delete", "javascript:void(0)",:class => "leadAction leadDelete" %> 
				</td>
			</tr>
		</tbody>
	<% end %>
<% else %>
	 <tr>
		<td colspan="5"> No record found. </td>
	 </tr>
<% end %>
