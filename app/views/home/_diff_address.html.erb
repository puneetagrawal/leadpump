<span class="credit_fild_col mb2"><%= label_tag :Address %><%= text_field_tag "address[address]"%></span>
<span class="credit_fild_col mb2">
	<%= label_tag :City %><%= text_field_tag  "address[city]"%>
</span>
<span class="credit_fild_col mb2">
	<label class="credit_fild_col">Zip</label><%= text_field_tag "address[zip]"%>
</span>
<span class="credit_fild_col mb2"><label>State/Region</label><%= text_field_tag "address[state]"%></span>
<span class="credit_fild_col mb2">
	<%= label_tag :Phone %>
	<%= text_field_tag "address[phone]", "", class: "phone_text"%>
</span>
<span class="credit_fild_col mb2"><%= label_tag :Country %><%= country_select("address", "country", [ "United States" ])%>
 </span>
<div class="clear"></div>				
