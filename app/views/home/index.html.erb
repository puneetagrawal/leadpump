<span class="gaugestart">
    <%=render "dashboard_gauges_script"%>
</span>

<div class="container">
    <div class="row-fluid">
        <div class="span12">
            <h2>Sales Dashboard</h2>
        </div>
    </div>
    <div class="row-fluid">
        <%=render "sales_user_list"%>
        <div class="<%=current_user.isCompany ? 'span9' : 'span12'%>">
            <h2 class="heading">Sales Representatives Statistics</h2>
            <div class="salesRepData" id="sales_repo_change">
                <%=render "sales_gauge_data"%>
            </div>
            <div class="clear mb10"></div>
            <div class="row-fluid">
                <div class="span3 lead_gaug">
                    <%=render "home/leadgauge"%>
                </div>
                <div class="span9">
                    <h2 class="heading">Dashboard</h2>
                    <div class="dashboardpage">
                        <ul class="dashboardIcons borderbox">
                            <%=render "home/companyDashboard"%>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
function signUserOut() {
    WL.logout();
}
</script>
<!-- <button onclick='signUserOut()'>Click here to sign out!</button> -->
<div id="upgrade_terms" class="popup_container">
  <div style="z-index:99999;top:3%">
    <div style="font-size:20px;text-align:center;" id="dltmall" class="small_popup">
    <span style="font-weight:bold;">Are You Sure want to upgrade your current plan.
      <div style="margin-top: 100px;">
        <%=link_to "Upgrade", new_plan_path(:maxUsers => @upgrade_user), :class=>"pop_yellow_btn"%>
        <span class="pop_yellow_btn cancelFancybox">Cancel</span>
      </div>
    </span>
  </div>
  </div>
</div>

<%=render "common_templates/popupContainer"%>

