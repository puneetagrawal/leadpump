<%if @user.errors.present?%>
	<%if @cls == "trial_left_input"%>
		$(".leadform1").html("<%=escape_javascript(render 'common_templates/signup_user', cls: @cls)%>");
	<%else%>
		$(".leadform").html("<%=escape_javascript(render 'common_templates/signup_user', cls: @cls)%>");
	<%end%>
<%else%>
	$.fancybox.open({
		href: '#email_access',
		type: 'inline'
	});
	$(".small_popup").html('<%=escape_javascript(render partial: "common_templates/email_access")%>');
<%end%>