<div class="container" style="padding-top:40px;">
	<div class="row-fluid">
		<div class="span12">
		</div>
		<div class="row-fluid">
			<div class="step_main_col_new">
				<div class="step_main_col step_new">
      				<div class="step_wise_col step_1">
          				<span class="step_circle step_active"><span >1</span></span>
			            <div class="step_txt">
			         		<h3>STEP 1</h3>
			            </div>
          			</div>
          
			        <div class="step_wise_col step_2">
			          	<span class="step_circle"><span>2</span></span>
		         		<div class="step_txt">
		         			<h3>STEP 2</h3>
		              	</div>
	          		</div>
          
		          	<div class="step_wise_col step_3">
		          		<span class="step_circle"><span>3</span></span>
		         		<div class="step_txt">
		         			<h3>STEP 3</h3>
		              	</div>
		          	</div>
      			</div>

      			<%=render "common_templates/campaign_temps/waiver_print"%>
      			<div>
      				<%#= link_to "Print", print_waiver_details_path(user: @user,dy:@dayscount,lead:params[:lead] ,format: 'pdf'), :class=>"btn yellow"%>
      			</div>
			</div>
		</div>
	</div>
</div>

<script>
	$(".step_1").addClass('step_line');
	$(".step_1 .step_circle").addClass('step_active').addClass('step_passed');
	$(".step_2").addClass('step_line');
	$(".step_2 .step_circle").addClass('step_active').addClass('step_passed');
	$(".step_3 .step_circle").addClass('step_active');
	$(".step_3 .step_circle").addClass('step_active').addClass('step_passed');

	$('.datepicker').datepicker();
</script>