<script type="text/javascript">
$(document).ready(function () {
    initgauge();
});

function initgauge(){
    $('.gaugeContainer').jqxGauge({
        ranges: [{ startValue: 0, endValue: 55, style: { fill: '#4bb648', stroke: '#4bb648' }, endWidth: 5, startWidth: 1 },
        { startValue: 55, endValue: 110, style: { fill: '#fbd109', stroke: '#fbd109' }, endWidth: 10, startWidth: 5 },
        { startValue: 110, endValue: 165, style: { fill: '#ff8000', stroke: '#ff8000' }, endWidth: 13, startWidth: 10 },
        { startValue: 165, endValue: 220, style: { fill: '#e02629', stroke: '#e02629' }, endWidth: 16, startWidth: 13 }],
        ticksMinor: { interval: 5, size: '5%' },
        ticksMajor: { interval: 10, size: '9%' },
        value: 0,
        colorScheme: 'scheme05',
        animationDuration: 1200,
        width: 180
    }); 
    
    
    lead = "<%=@leads%>";
    count = "<%=@count%>";
    lead = lead ? parseInt(lead) : 0;
    g = count ? parseInt(count) + 10 : 160;
    m = count ? parseInt(count) + 20 : 50;
    t = count ? parseInt(count) + 30 : 90;
    n = count ? parseInt(count) + 30 : 50;
    $("#leadgauge").jqxGauge('value', lead);  
    $('#gaugeContainer').jqxGauge('value', g);
    $('#totalgrossgauge').jqxGauge('value', m);
    $('#memsoldgauge').jqxGauge('value', t);
    $('#newcashgauge').jqxGauge('value', n);

    $('#leadgauge').on('valueChanging', function (e) {
        $(this).siblings('.gaugeValue').text(Math.round(e.args.value) + ' kph');
    });
    $('#gaugeContainer').on('valueChanging', function (e) {
        $(this).siblings('.gaugeValue').text(Math.round(e.args.value) + ' kph');
    });
    $('#totalgrossgauge').on('valueChanging', function (e) {
        $(this).siblings('.gaugeValue').text(Math.round(e.args.value) + ' kph');
    });
    $('#memsoldgauge').on('valueChanging', function (e) {
        $(this).siblings('.gaugeValue').text(Math.round(e.args.value) + ' kph');
    });
    $('#newcashgauge').on('valueChanging', function (e) {
        $(this).siblings('.gaugeValue').text(Math.round(e.args.value) + ' kph');
    });
}
</script>