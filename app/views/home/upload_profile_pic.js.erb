<%if current_user.isCompany%>
	if($("#user_<%=current_user.id%>").length){
		$("#user_<%=current_user.id%>").html('<%=escape_javascript(render partial: "sales_user", :locals=>{:user=>current_user})%>');
	}
	else{
		$(".seeusergauge ul").prepend('<%=escape_javascript(render partial: "dashboard_user_view", :locals=>{:user=>current_user})%>');
	}
	$(".user_profile").html('<%=escape_javascript(render partial: "common_templates/wizard_step3")%>');
	$(".step_2").addClass('step_line');
	$(".step_2 .step_circle").addClass('step_passed');
	$(".step_3 .step_circle").addClass('step_active');
<%else%>
$.fancybox.close();
<%end%>
