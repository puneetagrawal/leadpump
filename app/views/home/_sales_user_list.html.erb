<%if current_user.isCompany%>
    <div class="span3">
        <div class="salesRep borderbox">
            <h2 class="heading">Team Member</h2>
            <div class="list border borderbox seeusergauge">
                <ul>
                    <%if @users.present? %>
                        <%@users.each do |user|%>
                        <li id="user_<%=user.id%>">
                            <%=render "sales_user", :user=>user%>
                        </li>
                        <%end%>
                    <%else%>
                        <li>No sales users present.</li>
                    <%end%>
                </ul>
            </div>
        </div>
    </div>
<%end%>