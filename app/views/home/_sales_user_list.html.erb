<%#if current_user.isCompany%>
    <div class="wdt20_col">
        <div class="salesRep border grad_box_col">
            <h2 class="heading">TEAM MEMBERS</h2>
            <div class="list borderbox seeusergauge">
                <ul>
                    <%if @users.present? %>
                        <%@users.each do |user|%>
                            <%=render "dashboard_user_view", :user=>user%>
                        <%#if !current_user.isCompany%>
                            <!-- <span> -->
                                <!-- <a class="btn yellow submitlogo leadAction" style="font-size: 10px;min-width: 42px;padding: 2px 20px;">Upload Pic</a> -->
                                <%#= form_for 'picture', :url => '/createpic' ,:html => { :id => "c_logo",:class=>"uid_#{user.id}", :style=>"margin:0px;" } do |form| %>
                                  <%#= hidden_field_tag :u_id, user.id%>
                                  <%#= hidden_field_tag :home_dash, user.id%>
                                  <%#= form.file_field :avatar, :class=>"file_#{user.id}",:style =>"display:none" %>
                                <%# end %>
                            <!-- </span> -->
                        <%#end%>
                        <%end%>
                    <%else%>
                        <li>No sales users present.</li>
                    <%end%>
                </ul>
            </div>
        </div>
    </div>
<%#end%>
