<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>name</th>
  </tr>
<%if@users.size > 0 %>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
    </tr>
  <% end %>
  <% end %>
</table>

