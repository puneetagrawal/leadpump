
<div class="modal-header">
        <button type="button" class="close cancelFancybox" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>Invite Friends Via Email</h3>
    </div>
    <div class="modal-body">
        <table class="table table-striped">
            <thead>
                <tr><td>#</td><td>Name</td><td>Email</td></tr>
            </thead>
            <tbody>
              <%if @contacts.present?%>
                <% @contacts.each do |contact| %>
                  <tr>
                    <td><%=check_box_tag "freindId_#{contact.id}", 'yes', false, :class => 'gmailContactChekbox' %></td>
                    <td class="email_label"><%=contact.email%></td>
                    <td><%=contact.name.humanize%></td>
                  </tr>
                <%end%>
              <%end%>
            </tbody>
        </table>
    </div>
    <div class="modal-footer">
        <p class="sendReferralEmailBtn"><a class="btn yellow" href="javascript:void(0)">Send Invitations</a></p>
    </div>
