<% start_range = @planPerUser.present? ? @planPerUser.user_range.start_range : 1%>
<% end_range = @planPerUser.present? ? @planPerUser.user_range.end_range : 200%>
<span class="credit_fild_col mb2">
	<label>Users</label>
	<%= select_tag "discountOnUsers", options_for_select((start_range..end_range).collect{ |dl| [dl, dl] }),:class => "moreUserLink input-small"%>
	<span class="feild_info">All user must be on same plan.</span>
</span>
<span class="credit_fild_col mb2">
	<label>No of Locations</label>
	<span id="locationSelectBox">
		<%= select_tag "no_of_locations", options_for_select((1..end_range).collect{ |dl| [dl, dl] }), :class => "input-small" %>
		<span class="feild_info">Must have one user per location.</span>
	</span>
</span>