
<div class="modal-header">
        <button type="button" class="close cancelFancybox" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>Lead</h3>
    </div>
    <div class="modal-body">
        <table class="table table-striped">
            <!-- <thead>
                <tr><td>#</td><td>Name</td><td>Email</td></tr>
            </thead> -->
            <tbody>
                <tr><td>FistName</td><td>-</td><td><%=@lead.name%></td></tr>
                <tr><td>LastName</td><td>-</td><td><%=@lead.lname%></td></tr>
                <tr><td>Email</td><td>-</td><td><%=@lead.email%></td></tr>
                <tr><td>Phone</td><td>-</td><td><%=@lead.phone%></td></tr>
                <tr><td>Address</td><td>-</td><td><%=@lead.address%></td></tr>
                <tr><td>Reffered-By</td><td>-</td><td><%=@lead.refferred_by%></td></tr>
                <tr><td>Lead-Source</td><td>-</td><td><%=@lead.lead_source%></td></tr>
                <tr><td>Goals</td><td>-</td><td><%=@lead.goal%></td></tr>
                <tr><td>Guest-Pass-Issued</td><td>-</td><td><%=@lead.guest_pass_issued%></td></tr>
                <tr><td># of days</td><td>-</td><td><%=@lead.no_of_days%></td></tr>
                <tr><td>Potential-Enrolment</td><td>-</td><td><%=@lead.enrolment_value%></td></tr>
                <tr><td>Potential-Dues</td><td>-</td><td><%=@lead.dues_value%></td></tr>
                <tr><td>Notes</td><td>-</td><td><%=@lead.notes%></td></tr>
            </tbody>
        </table>
    </div>
    <div class="modal-footer">
        <a href="javascript:void(0)" class="btn yellow cancelFancybox">Done</a>
        <!-- <a href="javascript:void(0)" data-dismiss="modal" class="btn yellow cancelFancybox">Cancel</a> -->
    </div>